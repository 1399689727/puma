<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
	http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">


	<bean id="database1" class="com.dianping.puma.filter.DbTbList">
		<property name="dbName" value="swallow" />
		<property name="tbNameList">
			<list>
				<value>table1</value>
			</list>
		</property>
	</bean>

	<bean id="databases1" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<ref bean="database1" />
			</list>
		</constructor-arg>
	</bean>
	
	<bean id="defaulteventfilter" class="com.dianping.puma.filter.DefaultEventFilter">
		<property name="databases" ref="databases1">
		</property>
	</bean>


	

</beans>